<h1>Listing pages</h1>
<%= image_tag "/assets/images/rails.png" %>
<table>
  <tr>
    <th>User</th>
    <th>Active</th>
    <th>Height</th>
    <th>Title</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @pages.each do |page| %>
  <tr id="page_<%= page.id %>">
    <td><%= page.user_id %></td>
    <td><%= page.active %></td>
    <td><%= page.height %></td>
    <td><%= page.title %></td>
    <td><%= link_to 'Show', page %></td>
    <td><%= link_to 'Edit', edit_page_path(page) %></td>
    <td><%= link_to 'Destroy', page, :confirm => 'Are you sure?', :method => :delete, :remote => true %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Page', new_page_path %>
