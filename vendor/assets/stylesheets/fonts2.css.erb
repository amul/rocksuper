@font-face {
    font-family: 'ArimoRegular';
    src: url('<%= asset_path('arimo-regular-webfont.eot') %>');
    src: url('<%= asset_path('arimo-regular-webfont.eot?#iefix') %>') format('embedded-opentype'),
         url('<%= asset_path('arimo-regular-webfont.woff') %>') format('woff'),
         url('<%= asset_path('arimo-regular-webfont.ttf') %>') format('truetype'),
         url('<%= asset_path('arimo-regular-webfont.svg') %>#ArimoRegular') format('svg');
    font-weight: normal;
    font-style: normal;
}
@font-face {
    font-family: 'Univers59UltraCondensedRg';
    src: url('<%= asset_path('univers-ultracondensed-webfont.eot') %>');
    src: url('<%= asset_path('univers-ultracondensed-webfont.eot?#iefix') %>') format('embedded-opentype'),
         url('<%= asset_path('univers-ultracondensed-webfont.woff') %>') format('woff'),
         url('<%= asset_path('univers-ultracondensed-webfont.ttf') %>') format('truetype'),
         url('<%= asset_path('univers-ultracondensed-webfont.svg') %>#Univers59UltraCondensedRg') format('svg');
    font-weight: normal;
    font-style: normal;
}